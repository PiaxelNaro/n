{
  "name": "napoleon",
  "version": "0.0.0",
  "author": "Napoleon",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-actions": "^1.1.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "autoprefixer": "^7.1.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^0.28.4",
    "empty": "^0.10.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.1.2",
    "glob": "^7.1.2",
    "husky": "^0.13.4",
    "inquirer": "^3.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "jest-serializer-enzyme": "^1.0.0",
    "lint-staged": "^3.6.0",
    "node-sass": "^4.5.3",
    "nsp": "^2.6.3",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.4.4",
    "react-addons-test-utils": "^15.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "es6-promise-debounce": "^1.0.1",
    "fetch-ponyfill": "^4.0.0",
    "gettext-extractor": "^2.1.0",
    "highcharts": "^5.0.12",
    "hoist-non-react-statics": "^1.2.0",
    "http-proxy": "^1.16.2",
    "is-node": "^1.0.2",
    "jed": "^1.1.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "next": "^2.4.3",
    "next-redux-wrapper": "^1.1.2",
    "po2json": "^0.4.5",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-addons-shallow-compare": "^15.5.2",
    "react-click-outside": "tj/react-click-outside",
    "react-dates": "^10.1.1",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-modal": "^1.7.7",
    "react-portal": "^3.1.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "reselect": "^3.0.1",
    "styled-components": "^2.0.1",
    "styled-normalize": "^2.0.0",
    "universal-cookie": "^2.0.8"
  },
  "scripts": {
    "build": "next build",
    "analyze-bundle": "webpack-bundle-analyzer .next/stats.json",
    "start": "next start",
    "prepublish": "nsp check",
    "pretest": "flow-typed install && flow && eslint --ignore-path .gitignore .",
    "storybook": "start-storybook -p 6006 -s .",
    "test": "jest --coverage",
    "build-storybook": "build-storybook",
    "build-locale": "node bin/locale/build",
    "pull-locale": "node bin/locale/build --pull",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ignore-path .gitignore",
      "prettier --write --single-quote --trailing-comma es5 --tab-width=4 --print-width=90",
      "git add"
    ]
  },
  "repository": "git@github.com:napoleon/napoleon.git",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/css",
      "\\.(jpe?g|png|gif)$": "<rootDir>/__mocks__/image"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/\\.next",
      "<rootDir>/\\.flow-typed",
      "<rootDir>/\\flow-typed",
      "<rootDir>/\\.npm",
      "<rootDir>/\\node_modules"
    ]
  },
  "private": true
}
